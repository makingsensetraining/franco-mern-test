  "scripts": {
    "start": "npm-run-all --parallel open:src lint:watch test:watch",
    "open:src": "babel-watch api/server.js",
    "lint": "node_modules/.bin/esw webpack.config.* app tools",
    "ux:init": "npm-run-all ux:copy ux:install",
    "ux:copy": "ncp ./node_modules/msuxf ./ux",
    "ux:install": "cd ./ux && npm install",
    "ux:watch": "gulp --cwd ux --gulpfile ux/gulpfile.js watch",
    "ux:build": "gulp --cwd ux --gulpfile ux/gulpfile.js build",
    "lint:watch": "npm run lint -- --watch",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "open:coverage": "npm run test:coverage && opn ./coverage/lcov-report/index.html"
  },
